# Build targets

gateJAOTCWeekly : ${gateJAOTC} ${graalWeekly} {
  environment : {
    JAOTC_TEST_GROUP: "weekly"
  }
  run : [
    ${gateCmd} ["build,jaotc"]
  ]
}

builds += [
  ${gateJAOTC}       ${oraclejdk15} ${gateLinuxAMD64} {name: "gate-compiler-jaotc-openjdk-15-linux-amd64", timelimit:   "30:00"}
  ${gateJAOTCWeekly} ${oraclejdk15} ${gateLinuxAMD64} {name: "weekly-jaotc-openjdk-15-linux-amd64",        timelimit: "1:20:00"}
]
